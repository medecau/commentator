#!/usr/bin/python
# -*- coding: utf-8 -*-
import sys
import clint
import commentator

def main(argv):
    for arg in clint.args.all:
        d = commentator.comment(arg)
        print d.file
        for m in d.members:
            if m.isroutine:
                print '%s - %s' % (m.name, m.argspec)

if __name__ == '__main__':
    main(sys.argv)